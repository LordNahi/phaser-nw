{
  "name": "phaser-nw-template",
  "version": "1.0.0",
  "description": "Template Phaser/nw Project",
  "main": "dist/index.html",
  "window": {
    "title": "Phaser/nw Template",
    "toolbar": true,
    "width": 640,
    "height": 480,
    "resizable": false,
    "position": "center"
  },
  "scripts": {
    "start": "nw ./",
    "watch": "parcel watch game/index.html --public-url .",
    "build": "nwbuild --platforms win32,win64 --buildDir build/ dist/"
  },
  "author": "Nathan Nahi",
  "license": "ISC",
  "dependencies": {
    "phaser": "^3.55.2"
  },
  "devDependencies": {
    "nw": "^0.54.1-sdk",
    "nw-builder": "^3.5.7"
  }
}
